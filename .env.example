# API Configuration
VITE_API_BASE_URL=https://api.example.com

# Keycloak Configuration
VITE_KEYCLOAK_URL=https://sso.example.com
VITE_KEYCLOAK_REALM=backoffice
VITE_KEYCLOAK_CLIENT_ID=backoffice-spa
# Em dev: use proxy para evitar CORS (Vite encaminha para Keycloak). Deixe vazio ou remova em prod.
VITE_KEYCLOAK_PROXY_PATH=/auth-proxy

# Auth Redirects (devem estar cadastradas no client do Keycloak)
VITE_AUTH_REDIRECT_URI=http://localhost:3000/auth/callback
# URI após logout — cadastre em "Valid post logout redirect URIs" no Keycloak
VITE_AUTH_POST_LOGOUT_REDIRECT_URI=http://localhost:3000/auth/login
